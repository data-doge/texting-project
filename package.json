{
  "name": "texting-project",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test.js | smokestack | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/data-doge/texting-project"
  },
  "author": "data-doge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/data-doge/texting-project/issues"
  },
  "homepage": "https://github.com/data-doge/texting-project",
  "dependencies": {
    "copy-to-clipboard": "^2.0.0",
    "jquery": "^2.2.3",
    "json-markup": "^0.1.7",
    "keystroke-recorder": "^2.3.0",
    "lodash.last": "^3.0.0",
    "tocktimer": "^1.0.10"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "gh-pages": "^0.3.0",
    "smokestack": "^3.3.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  }
}
